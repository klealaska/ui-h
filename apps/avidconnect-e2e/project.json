{
  "name": "avidconnect-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/avidconnect-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/avidconnect-e2e/cypress.config.ts",
        "devServerTarget": "avidconnect:serve:dev",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "avidconnect:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/avidconnect-e2e/**/*.{js,ts}"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["type:e2e"],
  "implicitDependencies": ["avidconnect"]
}

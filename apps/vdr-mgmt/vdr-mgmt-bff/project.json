{
  "name": "vdr-mgmt-bff",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/vdr-mgmt/vdr-mgmt-bff/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/vdr-mgmt/vdr-mgmt-bff",
        "main": "apps/vdr-mgmt/vdr-mgmt-bff/src/main.ts",
        "tsConfig": "apps/vdr-mgmt/vdr-mgmt-bff/tsconfig.app.json",
        "assets": ["apps/vdr-mgmt/vdr-mgmt-bff/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "apps/vdr-mgmt/vdr-mgmt-bff/webpack.config.js",
        "generatePackageJson": true
      },
      "configurations": {
        "development": {},
        "production": {},
        "mock": {
          "fileReplacements": [
            {
              "replace": "apps/vdr-mgmt/vdr-mgmt-bff/src/environments/environment.ts",
              "with": "apps/vdr-mgmt/vdr-mgmt-bff/src/environments/environment.mock.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "vdr-mgmt-bff:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "vdr-mgmt-bff:build:production"
        },
        "mock": {
          "buildTarget": "vdr-mgmt-bff:build:mock"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/vdr-mgmt/vdr-mgmt-bff/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/vdr-mgmt/vdr-mgmt-bff/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": ["type:bff", "scope:vdr-mgmt"]
}

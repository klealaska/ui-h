{
  "name": "shared-data-access-content",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/data-access/content/src",
  "prefix": "ui-coe",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["dist/libs/shared/data-access/content"],
      "options": {
        "project": "libs/shared/data-access/content/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/data-access/content/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/data-access/content/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/libs/shared/data-access/content"],
      "options": {
        "jestConfig": "libs/shared/data-access/content/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/data-access/content/**/*.ts",
          "libs/shared/data-access/content/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared", "scope:content", "type:data-access"]
}

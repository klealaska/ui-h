{
  "name": "demo-game-feature",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/demo/game/feature/src",
  "prefix": "demo",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "options": {
        "tsConfig": "libs/demo/game/feature/tsconfig.lib.json",
        "project": "libs/demo/game/feature/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/demo/game/feature/tsconfig.lib.prod.json"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/demo/game/feature/src/**/*.ts",
          "libs/demo/game/feature/src/**/*.html"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/demo/game/feature"],
      "options": {
        "jestConfig": "libs/demo/game/feature/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:demo", "type:feature"]
}

{
  "name": "tenant-shared-util",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/tenant/shared/util/src",
  "prefix": "ui-coe",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/libs/tenant/shared/util"],
      "options": {
        "project": "libs/tenant/shared/util/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/tenant/shared/util/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/tenant/shared/util/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/tenant/shared/util"],
      "options": {
        "jestConfig": "libs/tenant/shared/util/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/tenant/shared/util/**/*.ts", "libs/tenant/shared/util/**/*.html"]
      }
    }
  },
  "tags": ["type:util", "scope:shared"]
}

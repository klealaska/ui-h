{
  "name": "pay-shared-types",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/pay/shared/types/src",
  "prefix": "ui-coe",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/libs/pay/shared/types"],
      "options": {
        "project": "libs/pay/shared/types/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/pay/shared/types/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/pay/shared/types/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/pay/shared/types"],
      "options": {
        "jestConfig": "libs/pay/shared/types/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/pay/shared/types/src/**/*.ts",
          "libs/pay/shared/types/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared", "type:types"]
}

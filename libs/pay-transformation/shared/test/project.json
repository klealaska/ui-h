{
  "name": "pay-transformation-shared-test",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/pay-transformation/shared/test/src",
  "prefix": "ui-coe",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/pay-transformation/shared/test/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/pay-transformation/shared/test/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/pay-transformation/shared/test/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/pay-transformation/shared/test/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/pay-transformation/shared/test/**/*.ts",
          "libs/pay-transformation/shared/test/**/*.html"
        ]
      }
    }
  },
  "tags": ["type:lib", "scope:pay-transformation"]
}
